{
  "name": "autostock-backend",
  "version": "0.1.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:seed": "ts-node prisma/seed.ts",
    "test": "npm run build && jest --runInBand --passWithNoTests",
    "deploy:build": "npm ci && npm run build && npm run prisma:generate",
    "deploy:migrate": "npm run prisma:migrate:prod",
    "deploy:full": "npm run deploy:build && npm run deploy:migrate && npm run prisma:seed",
    "postinstall": "npm run prisma:generate",
    "docker:build": "docker build -t autostock-backend .",
    "docker:run": "docker-compose up -d",
    "docker:logs": "docker-compose logs -f app",
    "docker:stop": "docker-compose down"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "express-rate-limit": "^7.4.0",
    "jsonwebtoken": "^9.0.0",
    "zod": "^3.20.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.19.0",
    "@types/supertest": "^2.0.16",
    "jest": "^29.0.0",
    "prisma": "^5.0.0",
    "supertest": "^6.3.4",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
